Config { font = "-misc-fixed-*-*-*-*-10-*-*-*-*-*-*-*"
       , bgColor = "#000000"
       , fgColor = "#BFBFBF"
       , xPos = 0
       , yPos = 0
       , width = 1024
       , height = 15
       , align = "right"
       , refresh = 10
       , commands = [ Run Cpu [] 10
                    , Run Weather "LIPB" [] 36000
                    , Run Battery [] 10
                    , Run HelloWorld
                    ]
       , sepChar = "%"
       , template = "%cpu% | %helloWorld% | %LIPB% | <fc=yellow>%date%</fc> | %battery%"
       }
